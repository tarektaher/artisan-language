{
    "name": "tarektaher/artisan-language",
    "description": "add artisan commands for language",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^11"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/tarektaher/artisan-language",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Tarektaher\\ArtisanLanguage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tarektaher\\ArtisanLanguage\\ArtisanLanguageServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "tarektaher",
            "email": "tarektaher@live.fr"
        }
    ],
    "minimum-stability": "dev"
}
